{
  "name": "logpile",
  "version": "1.0.1",
  "description": "ðŸªµ A simple & tiny logging library with supercharged search features",
  "repository": "skoshx/logpile",
  "main": "./dist/index.cjs",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=12.13"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "NODE_NO_WARNINGS=1 ava",
    "build": "rimraf dist && rollup -c",
    "prepublishOnly": "npm run build",
    "prettier-check": "prettier --check .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "release": "np"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ]
  },
  "prettier": {
    "endOfLine": "lf",
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "keywords": [
    "utility",
    "typescript",
    "logger",
    "logging",
    "search",
    "extendable",
    "simple",
    "tiny"
  ],
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "ava": "^4.1.0",
    "esbuild": "^0.14.31",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "ms": "3.0.0-canary.1"
  }
}
